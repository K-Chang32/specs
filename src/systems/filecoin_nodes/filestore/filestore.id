type Path string

type File struct {
  Path() Path
  Size() int
  Seek(offset int) error
  Read(buf Bytes) struct { size int, e error }
  Write(buf Bytes) struct { size int, e error }
  Close() error
}

type Store struct {
  Open(p Path) union{ f File, e error }
  Create(p Path) union{ f File, e error }
  Delete(p Path) error

  // maybe add:
  // Copy(SrcPath, DstPath)
}
